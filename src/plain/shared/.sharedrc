{
  "settings": {
    "browserSupport": ["> 1%", "not ie 11"],
    "bundleName": "${content.bundleName}_${settings.size.width}x${settings.size.height}",
    "optimizations": {
      "css": true,
      "image": true,
      "html": true,
      "js": true
    },
    "contentSource": {
      "url": "https://docs.google.com/spreadsheets/d/1f-BxXd5-yWXrHwZZHJHmfRZHG8vqtSsQ8V0AUjPQXeA/edit#gid=0"
    },
    "useOriginalFileNames": false,
    "entry": {
      "js": "./script/main.js",
      "html": "./index.hbs"
    },
    "fonts": [
      {
        "sources": [
          "./fonts/OpenSans-Regular.woff",
          "./fonts/OpenSans-Regular.woff2"
        ],
        "subset": {
          "glyphs": [
            "content.title"
          ]
        }
      },
      {
        "sources": [
          "./fonts/OpenSans-Bold.woff",
          "./fonts/OpenSans-Bold.woff2"
        ],
        "subset": {
          "glyphs": [
            "content.cta"
          ]
        }
      }
    ]
  },
  "content": {
    "css": "./css/style.css",
    "defaultFonts": [
      "OpenSans",
      "OpenSansBold"
    ],
    "defaultFontUrl": "./css/fonts.css",
    "clickTag": "https://www.google.com",
    "title": "Welcome to this Banner!",
    "logo": "./img/logo.svg",
    "cta": "CLICK HERE",
    "bgcolor": "#FFFFFF"
  }
}
